---
interface Props {
	class?: string;
	style?: string;
    height?: string;
}

const { class: className, style, height = "40px" } = Astro.props;
---

<img 
    src="/assets/SESSR_logo_concept.png" 
    alt="SESSR Logo" 
    class={className}
    style={`${style}; height: ${height}; width: auto; vertical-align: middle;`}
/>
